# Antigen
source ~/.zsh/antigen.zsh

# Env Variables
source ~/.zsh/env.zsh

# Antigen plugins
source ~/.zsh/antigenrc

# Aliases
source ~/.zsh/aliases

# History
source ~/.zsh/history

# Helper Functions
source ~/.zsh/helpers.zsh
export NVS_HOME="$HOME/.nvs"
[ -s "$NVS_HOME/nvs.sh" ] && . "$NVS_HOME/nvs.sh"
